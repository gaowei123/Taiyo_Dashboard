    <script type="text/javascript">
        //调整 body 的padding top 适应窗口缩放.
        $(document).ready(function () {
            setBodyMarginTop($(document.body).outerWidth(true));
        })
        $(window).resize(function () {
            setBodyMarginTop($(document.body).outerWidth(true));
        });


        /*解决 navbar-fixed-top 遮挡下方div*/
        function setBodyMarginTop(currentBrowserWidth) {
            if (currentBrowserWidth > 1120) {
                //正常宽度
                $("body").css("paddingTop", 56);
            } else if (currentBrowserWidth < 754) {
                //754宽度内 logo图片过长， 导致按钮换行
                $("body").css("paddingTop", 56);
                $("#imglogo").width(300);
            }
            else {
                //1120-754宽度内 菜单栏自动换行 导致navbar变宽
                $("body").css("paddingTop", 106);
            }
        }

        //将所有input框的 autocomplete off.
        $(function () {
            $('input:not([autocomplete]),textarea:not([autocomplete]),select:not([autocomplete])').attr('autocomplete', 'off');
        });
    </script>
