
@{
    ViewBag.Title = "Monthly Highest Reject";
}




@*时间框组件*@
<script src="~/Plugins/bootstrap-datetimepicker-master/js/bootstrap-datetimepicker.js"></script>
<link href="~/Plugins/bootstrap-datetimepicker-master/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />



<div class="container container-fluid">



    <div class="row titleRow">
        <img class="titleImg" src="~/Resources/Images/headericon.gif" />
        <span class="titleText">Mouldling Monthly Highest Reject Report</span>
    </div>

    <div class="row searchingBar">
        <div class="col-md-12 panel panel-default searchingPanel" style="padding-top:10px;">
            <div class="row form-inline">
                <div class="col-md-3">
                    <label for="txtDateFrom">Date From:</label>
                    <input type="text" class="form-control" id="txtDateFrom" CssClass="form-control formDateTimePicker" data-date-format="yyyy-mm-dd" placeholder="Please choose date" style="width:50%;" />
                </div>
                <div class="col-md-3">
                    <label for="txtDateTo">Date To:</label>
                    <input type="text" class="form-control" id="txtDateTo" CssClass="form-control formDateTimePicker" data-date-format="yyyy-mm-dd" placeholder="Please choose date" style="width:50%;" />
                </div>
                <div class="col-md-3">
                    @*<label for="ddlMachineID">Machine ID:</label>
                        <select id="ddlMachineID" class="form-control selectpicker" data-ng-style="btn-info"></select>*@
                </div>
                <div class="col-md-3" align="right">
                    <input type="button" class="btn btn-success" value="Generate" onclick="search();" />
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12" style="padding:0px;">
            <table id="tableParts" data-toggle="table" data-height="380"></table>
        </div>
    </div>

    <div class="row" style="margin-top:10px;">
        <div class="col-lg-12" style="padding:0px;">
            <table id="tableDefects" data-toggle="table" data-height="380"></table>
        </div>
    </div>

</div> 

<script src="~/plugins/bootstrap-table-1.15.3/dist/bootstrap-table.min.js"></script>
<link href="~/plugins/bootstrap-table-1.15.3/dist/bootstrap-table.min.css" rel="stylesheet" />

<script src="~/Dashboard CSS JS/JS/MOULD_MonthlyHighestReject.js"></script>
